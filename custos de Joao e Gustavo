#Definindo as listas de gastos
gastos_joao = {300, 500, 200, 800}
gastos_gustavo = {200, 400, 500, 700}

#Calculando o total de gastos
total_joao =sum(gastos_joao)
total_gustavo = sum(gastos_gustavo)

#Comparando os gastos 
if total_joao>total_gustavo:
    print("joao gastou mais dinheiro ao longo do mês.")
elif total_gustavo>total_joao:
    print("gustavo gastou mais dinheiro ao longo do mês.")
else:
    print("joao e gustavo gastaram a mesma quantia")
